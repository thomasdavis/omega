{
  "name": "bot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc && npm run copy-public",
    "copy-public": "mkdir -p dist/public && cp -r public/* dist/public/",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.67",
    "@slidev/cli": "^0.50.0",
    "ai": "6.0.0-beta.99",
    "discord.js": "^14.24.2",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "playwright-chromium": "^1.48.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.8.6",
    "@vitest/coverage-v8": "^2.1.8",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  }
}
