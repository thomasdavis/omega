{
  "name": "bot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "prebuild": "node scripts/generate-deployment-info.js",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "mkdir -p dist/public && cp -r public/* dist/public/ && cp omega-manifest.json dist/",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.67",
    "@google/generative-ai": "^0.21.0",
    "@libsql/client": "^0.14.0",
    "@octokit/rest": "^21.0.0",
    "@slidev/cli": "^0.50.0",
    "ai": "6.0.0-beta.99",
    "discord-api-types": "^0.38.35",
    "discord.js": "^14.24.2",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "lib0": "^0.2.114",
    "mime": "^4.0.0",
    "minimatch": "^10.1.1",
    "node-cron": "^3.0.3",
    "openai": "^4.104.0",
    "playwright-chromium": "^1.48.0",
    "pusher": "^5.2.0",
    "undici": "^7.2.0",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.8.6",
    "@types/node-cron": "^3.0.11",
    "@vitest/coverage-v8": "^2.1.8",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  }
}
